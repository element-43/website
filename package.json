{
  "name": "element-43-website",
  "private": true,
  "version": "0.1.0",
  "description": "Website for Element 43.",
  "main": "src/server/app.js",
  "author": {
    "name": "Kieran O'Neill",
    "email": "kieran.oneill@element43.xyz",
    "url": "https://element43.xyz"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kieranroneill/element-43-website"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "engines": {
    "node": "6.11.0"
  },
  "scripts": {
    "babel": "babel",
    "build": "concurrently \"npm run build:server\" \"npm run build:common\" \"npm run build:client\"",
    "build:client": "node -r babel-register node_modules/.bin/webpack --config webpack/webpack.prod.config.js --progress",
    "build:common": "babel src/common -d dist/common",
    "build:server": "babel src/server -d dist/server --ignore test.js",
    "concurrently": "concurrently",
    "eslint": "eslint",
    "lint": "eslint --ext .js --ext .jsx . && stylelint -q './src/**/*.css'",
    "mocha": "mocha",
    "nodemon": "nodemon",
    "nyc": "nyc",
    "postinstall": "(cp -n .env.example .env || true)",
    "start": "concurrently \"npm run start:server\" \"npm run start:client\"",
    "start:client": "node -r babel-register node_modules/.bin/webpack-dev-server --config webpack/webpack.dev.config.js --progress",
    "start:server": "nodemon src/server/app",
    "stylelint": "stylelint",
    "test": "npm run build:client && nyc mocha -- './src/**/*.test.{js,jsx}'"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "bluebird": "^3.4.7",
    "body-parser": "^1.16.1",
    "contentful": "^4.5.0",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "express-validator": "^3.2.1",
    "helmet": "^3.4.0",
    "http-codes": "^1.0.0",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "normalize.css": "^6.0.0",
    "prop-types": "^15.5.10",
    "react": "^15.6.0",
    "react-cookies": "^0.1.0",
    "react-css-modules": "^4.3.0",
    "react-dom": "^15.6.0",
    "react-helmet": "^5.1.3",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "redux": "^3.7.2",
    "velocity-animate": "^1.5.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^4.0.2",
    "chai-enzyme": "^0.7.1",
    "clean-webpack-plugin": "^0.1.16",
    "concurrently": "^3.5.0",
    "css-loader": "^0.28.4",
    "enzyme": "^2.8.2",
    "eslint": "^4.0.0",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "favicons-webpack-plugin": "^0.0.7",
    "file-loader": "^0.11.2",
    "handlebars": "^4.0.10",
    "handlebars-loader": "^1.5.0",
    "html-webpack-plugin": "^2.28.0",
    "ignore-styles": "^5.0.1",
    "jsdom": "^11.0.0",
    "lodash-webpack-plugin": "^0.11.4",
    "mocha": "^3.4.2",
    "mocha-junit-reporter": "^1.13.0",
    "mocha-multi-reporters": "^1.1.4",
    "node-mocks-http": "^1.6.4",
    "node-sass": "^4.5.3",
    "nyc": "^11.0.3",
    "postcss-loader": "^1.3.1",
    "react-addons-test-utils": "^15.6.0",
    "react-hot-loader": "^1.3.1",
    "redux-mock-store": "^1.2.3",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.5",
    "sinon": "^2.3.4",
    "style-loader": "^0.18.2",
    "stylelint": "^7.11.0",
    "supertest": "^3.0.0",
    "url-loader": "^0.5.9",
    "webpack": "^2.6.1",
    "webpack-combine-loaders": "^2.0.3",
    "webpack-dev-server": "^2.4.5",
    "webpack-notifier": "^1.5.0",
    "webpack-visualizer-plugin": "^0.1.11"
  }
}
