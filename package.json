{
  "name": "element-43-website",
  "private": true,
  "version": "0.1.0",
  "description": "Website for Element 43 Consulting Limited.",
  "main": "src/web/index.ts",
  "author": {
    "name": "Kieran O'Neill",
    "email": "kieran.oneill@element43.xyz",
    "url": "https://element43.xyz"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kieranroneill/element-43-website"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "engines": {
    "node": ">=8.11.2"
  },
  "scripts": {
    "build": "yarn build:api && yarn build:web",
    "build:api": "rm -rf dist/api dist/common && tsc --project \"__typescript__/tsconfig.api.json\"",
    "build:web": "node -r @babel/register node_modules/.bin/webpack --config \"__webpack__/webpack.config.js\" --progress",
    "lint": "eslint --ext .js .",
    "postinstall": "(cp -n .env.example .env || true)",
    "prettier": "prettier --config ./.prettierrc --write \"**/*.{js,json,jsx,ts,tsx}\"",
    "start": "cross-env NODE_ENV=\"development\" concurrently \"yarn start:api\" \"yarn start:web\"",
    "start:api": "nodemon \"src/api/index\"",
    "start:web": "node -r dotenv/config -r @babel/register node_modules/.bin/webpack-dev-server --config \"__webpack__/webpack.dev.config.js\" --progress",
    "test": "cross-env JEST_JUNIT_OUTPUT=\"junit/test-results.xml\" jest --config=\"__test__/jest.config.js\""
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "connected-react-router": "^6.4.0",
    "cookie-parser": "^1.4.3",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "express-validator": "^5.2.0",
    "helmet": "^3.12.1",
    "history": "^4.9.0",
    "http-status-codes": "^1.3.0",
    "lodash": "^4.17.10",
    "moment": "^2.22.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-helmet": "^5.2.0",
    "react-redux": "^7.0.3",
    "react-responsive": "^4.1.0",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.0",
    "styled-components": "^4.2.0",
    "velocity-animate": "^1.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/register": "^7.0.0",
    "@types/body-parser": "^1.17.0",
    "@types/cookie-parser": "^1.4.1",
    "@types/dotenv": "^4.0.3",
    "@types/enzyme": "^3.1.10",
    "@types/express": "^4.11.1",
    "@types/helmet": "^0.0.37",
    "@types/jest": "^23.0.0",
    "@types/lodash": "^4.14.109",
    "@types/node": "^10.3.0",
    "@types/react": "^16.3.16",
    "@types/react-dom": "^16.0.5",
    "@types/react-helmet": "^5.0.6",
    "@types/react-redux": "^7.0.8",
    "@types/react-responsive": "^3.0.1",
    "@types/react-router-dom": "^4.3.2",
    "@types/styled-components": "^4.1.14",
    "@types/supertest": "^2.0.4",
    "@types/velocity-animate": "^1.2.32",
    "autoprefixer": "^8.5.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.4.2",
    "clean-webpack-plugin": "^0.1.16",
    "concurrently": "^3.5.1",
    "cross-env": "^5.1.5",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "favicons-webpack-plugin": "^0.0.9",
    "file-loader": "^1.1.11",
    "handlebars": "^4.0.10",
    "handlebars-loader": "^1.5.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^2.1.0",
    "jest": "^22.4.4",
    "jest-junit": "^4.0.0",
    "jsdom": "^11.0.0",
    "lint-staged": "^8.1.5",
    "node-mocks-http": "^1.7.0",
    "nodemon": "^1.17.5",
    "postcss-loader": "^2.1.5",
    "prettier": "^1.17.0",
    "style-loader": "^0.21.0",
    "supertest": "^3.1.0",
    "tasksfile": "^5.1.0",
    "ts-jest": "^22.4.6",
    "ts-loader": "^4.3.1",
    "ts-node": "^6.1.0",
    "typescript": "^2.9.1",
    "url-loader": "^1.0.1",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.2.1",
    "webpack-notifier": "^1.5.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,json,jsx,ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  }
}
