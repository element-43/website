  "engines": {
    "node": ">=8.11.2"
  },
  "scripts": {
    "build": "yarn build:api && yarn build:web",
    "build:api": "rm -rf dist/api dist/common && tsc --project tsconfig.api.json",
    "build:web": "node -r babel-register node_modules/.bin/webpack --config webpack/webpack.config.js --progress",
    "lint": "eslint --ext .js .",
    "postinstall": "(cp -n .env.example .env || true)",
    "start": "cross-env NODE_ENV=\"development\" concurrently \"yarn start:api\" \"yarn start:web\"",
    "start:api": "nodemon src/api/index",
    "start:web": "node -r babel-register node_modules/.bin/webpack-dev-server --config webpack/webpack.dev.config.js --progress",
    "test": "cross-env JEST_JUNIT_OUTPUT=\"junit/test-results.xml\" jest --config=test/jest.config.js"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "express-validator": "^5.2.0",
    "helmet": "^3.12.1",
    "http-status-codes": "^1.3.0",
    "lodash": "^4.17.10",
    "moment": "^2.22.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.7",
    "react-responsive": "^4.1.0",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0",
    "styled-components": "^3.2.6",
    "velocity-animate": "^1.5.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/cookie-parser": "^1.4.1",
    "@types/dotenv": "^4.0.3",
    "@types/enzyme": "^3.1.10",
    "@types/express": "^4.11.1",
    "@types/helmet": "^0.0.37",
    "@types/jest": "^23.0.0",
    "@types/lodash": "^4.14.109",
    "@types/node": "^10.3.0",
    "@types/react": "^16.3.16",
    "@types/react-dom": "^16.0.5",
    "@types/react-helmet": "^5.0.6",
